{
    "name": "ReplyTube",
    "version": "1.0",
    "description": "YouTube Comments Extension for Disabled Comments.",
    "manifest_version": 2,

    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": []
        }
      ],

      "browser_action": {
        "default_icon": "icon.png",
        "default-popup": "popup.html"
      },

      "permissions":[
          "activeTab"
      ]
}

